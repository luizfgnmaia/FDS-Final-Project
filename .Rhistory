library(stringr)
library(dplyr)
arruma_data <- function(x) {
if(nchar(x) == 15) {
ano1 = str_sub(x, start = 7, end = 10)
ano2 = str_sub(x, start = 12, end = 15)
if(as.integer(str_sub(x, start = 4, end = 5)) >= 7) {
x = (paste0(str_sub(x, start = 1, end = 6), ano1))
} else {
x = (paste0(str_sub(x, start = 1, end = 6), ano2))
}
}
as.Date(x, "%d.%m.%Y")
}
